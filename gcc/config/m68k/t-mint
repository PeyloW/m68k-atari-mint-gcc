MULTILIB_OPTIONS = m68020-60/mcfv4e mshort
MULTILIB_MATCHES = m68000=mc68000 m68020-60=m68881 m68020-60=m68020 m68020-60=mc68020 m68020-60=m68020-40 m68020-60=m68030 m68020-60=m68040 m68020-60=m68060
MULTILIB_EXCEPTIONS = m68020-60/mshort mcfv4e/mshort

LIBGCC = stmp-multilib
INSTALL_LIBGCC = install-multilib
